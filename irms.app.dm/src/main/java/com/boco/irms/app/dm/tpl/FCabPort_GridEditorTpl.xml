<?xml version="1.0" encoding="UTF-8"?>
<!-- 交接箱 模块端子-->
<tpl>
	<editor name="IRMS.RMS.FCABPORT" classname="FCABPORT" title="交接箱端子">
		<columns>
		
			<column id="OBJECTID"  filter="all" categoryname="系统属性">OBJECTID</column>
			<column id="CUID"  filter="all" categoryname="系统属性">CUID</column>
			 
			<column id="LABEL_CN" filter="all" editable="true"
				categoryname="系统属性" >端子名称</column>
			<column id="RELATED_MODULE_CUID" editable="false" 
			   editor="dmcombox"
			   categoryname="系统属性">所属模块</column>
			<column id="SERVICE_STATE" filter="all" editor="enumbox@DMServiceState" 
				editable="true"   categoryname="业务">端子业务状态</column>
			<column id="PORT_TYPE"  filter="all" editable="true"  
				categoryname="业务">端子类型</column>
			<column id="PORT_LABEL"  filter="all"  categoryname="业务"
				editable="true">端子标签信息</column>
			<column id="IS_CONNECTED" editable="false"
			    editor="enumbox@BooleanType"
				categoryname="业务">端子与端子连接状态</column>
			<column id="IS_CONNECTED_TO_FIBER" editable="false"
			    editor="enumbox@BooleanType"
				categoryname="业务">端子与纤芯连接状态</column>	
			<column id="NUM_IN_MROW" editable="false"
				categoryname="业务">端子所在行</column>
			<column id="NUM_IN_MCOL" editable="false"
				categoryname="业务">端子所在列</column>			
		</columns>
		
	</editor>
</tpl>
			
    
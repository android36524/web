<?xml version="1.0" encoding="UTF-8"?>
<!-- 光终端盒 -->
<tpl>
	<editor name="IRMS.RMS.FIBERTERMINALBOX" classname="FIBER_JOINT_BOX" title="光终端盒">
		<columns>
			<column id="OBJECTID" filter="all" categoryname="系统属性">OBJECTID
			</column>
			<column id="CUID" filter="all" categoryname="系统属性">CUID</column>

			<column id="LABEL_CN" editable="true" categoryname="系统属性" nullable="true">名称
			</column>
			
			<column id="RELATED_DISTRICT_CUID" editable="true" editor="asyncombox@DMDF.DISTRICT" 
			      categoryname="基本属性" nullable="true" relationproperty="true">所属区域
			</column>

			<column id="OWNERSHIP" editable="true" nullable="true"
				editor="enumbox@DMCabOwnerShip" categoryname="基本属性">产权归属
			</column>
			
			<column id="LONGITUDE" editable="true" editor="double@0~180|6" categoryname="基本属性">显示经度
			</column>
			
			<column id="LATITUDE" editable="true" editor="double@0~90|6" categoryname="基本属性">显示纬度
		    </column>

			<column id="REAL_LONGITUDE" editable="true" editor="double@0~180|6" categoryname="基本属性">实际经度
			</column>
			
			<column id="REAL_LATITUDE" editable="true" editor="double@0~90|6" categoryname="基本属性">实际纬度
			</column>
			
			<column id="DESIGN_CAPACITY" editable="true" editor="long" categoryname="基本属性">设计容量(芯)
			</column>

			<column id="USED_CAPACITY" editable="true" editor="long" categoryname="基本属性">使用容量(芯)
			</column>

			<column id="INSTALL_CAPACITY" editable="true" editor="long" categoryname="基本属性">安装容量(芯)
			</column>

			<column id="FREE_CAPACITY" editable="true" editor="long" categoryname="基本属性">空闲容量(芯)
			</column>

			<column id="CAPACITY" editable="true" categoryname="基本属性">终端盒容量(芯)
			</column>
			
			<column id="RELATED_TEMPLATE_NAME" editable="true" categoryname="基本属性">关联模板名称
			</column>

			<column id="MODEL" editable="true" categoryname="基本属性">终端盒型号
			</column>

            <column id="KIND" editable="true" filter = "grid" editor="enumbox@DMFBoxKindType"
				categoryname="基本属性">设备类型
			</column>
				
			<column id="LABEL_DEV" editable="true" categoryname="基本属性">设备标识
			</column>

			<column id="SEQNO" editable="true" categoryname="基本属性">设备序列号
			</column>

			<column id="RELATED_VENDOR_CUID" editable="true" editor="enumbox@DeviceVendor"
				renderer="enumbox@DeviceVendor" categoryname="基本属性">设备供应商
			</column>

			<column id="SPECIAL_LABEL" editable="true" categoryname="基本属性">厂商特征值
			</column>
			
			<!-- 
			<column id="RELATED_LOCATION_CUID" editable="true" editor="asyncombox@SPACE.ACCESSPOINT"
				categoryname="基本属性" relationproperty="true">所属接入点
			</column>
			
			<column id="RELATED_ROOM_CUID" editable="true" editor="asyncombox@SPACE.ROOM"
				categoryname="基本属性" relationproperty="true">所属机房
			</column>
			 -->
			 
			<column id="RELATED_LOCATION_CUID" editable="true" categoryname="基本属性" relationproperty="true"
			    editor="menucombox@DM_POLE=电杆|DM_INFLEXION=拐点|DM_ROOM=机房|DM_ACCESSPOINT=接入点|DM_FIBER_CAB=交接箱">终端盒所在位置
			</column>

			<column id="UP_POINT_NAME" editable="true" nullable="true" categoryname="业务属性">引接点信息
			</column>
			
			<column id="ACCESS_SCENE" filter="all" editable="true" nullable="true"
				editor="enumbox@DMAccessSence" categoryname="业务属性">集客接入场景
		    </column>
			
			<column id="RES_OWNER" editable="true" categoryname="业务属性">所有权人
			</column>
			
			<column id="USERNAME" editable="true" categoryname="业务属性">使用单位
			</column>
			
			<column id="MAINT_DEP" editable="true" categoryname="业务属性">维护单位
			</column>
			
			<column id="LOCATION" editable="true" categoryname="业务属性">地址
			</column>
			
			<column id="PRESERVER" editable="true" categoryname="业务属性">维护人
			</column>

			<column id="PRESERVER_PHONE" editable="true" categoryname="业务属性">维护人联系电话
			</column>

			<column id="PRESERVER_ADDR" editable="true" categoryname="业务属性">维护人通信地址
			</column>

			<column id="MAINT_MODE" editable="true" editor="enumbox@DMMaintMode" nullable="true"
				categoryname="业务属性">维护方式
			</column>

			<column id="IS_USAGE_STATE" editable="true" editor="enumbox@BooleanType"
				categoryname="业务属性">是否正在使用
			</column>

			<column id="CREATOR" editable="true" categoryname="业务属性">建设单位
			</column>

			<column id="CREATTIME" editable="true" editor="date" categoryname="业务属性">建设日期
			</column>

			<column id="SETUP_TIME" editable="true" editor="date" categoryname="业务属性">入网时间
			</column>


			<column id="RELATED_PROJECT_CUID" editable="true" editor="dmcombox@service_dict_dm.DM_PROJECT_MANAGEMENT"
				categoryname="业务属性" relationproperty="true">所属工程
			</column>

			<column id="PROJECT_STATE" editable="false" editor="enumbox@DMProjectState"
				categoryname="业务属性">工程状态
			</column>
			
			<column id="RELATED_MAINT_CUID" editable="true" editor="dmcombox@service_dict_dm.DM_MAINT_MANAGEMENT"
				categoryname="业务属性" relationproperty="true">所属维护作业
			</column>

			<column id="MAINT_STATE" editable="false" editor="enumbox@MaintState"
				categoryname="业务属性">维护作业状态
			</column>

			<column id="BOSS_CODE" editable="true" categoryname="业务属性">boss编码
			</column>

			<column id="VP_LABEL_CN" editable="true" categoryname="业务属性">客户名称
			</column>

			<column id="REMARK" editable="true" categoryname="其他属性">备注
			</column>
			
			<column id="CREATOR" editable="false" categoryname="其他属性">录入人
			</column>
			
			<column id="CREATE_TIME" editable="false" editor="date" categoryname="其他属性">录入时间
			</column>

			<column id="LAST_MODIFY_TIME" editor="date" editable="false" categoryname="其他属性">最后修改时间
			</column>
			
		</columns>
		<cm>
			{
				KIND : {
					hidden : true,
				}
			}
		</cm>
		<sql>
			SELECT * FROM FIBER_JOINT_BOX
		</sql>
		<remote name="FiberBoxPropTemplateBO">
			<method type="query">IFiberJointBoxBO.getFiberJointBoxBySql</method>
			<method type="count">IFiberJointBoxBO.getFiberJointBoxBySql</method>
			<method type="insert">IFiberJointBoxBO.addFiberJointBoxs</method>
			<method type="update">IFiberJointBoxBO.modifyFiberJointBoxs</method>
			<method type="delete">IFiberJointBoxBO.deleteFiberJointBoxs</method>
			<method type="editorMetaPluginBoName">DefaultEditorMetaPluginBO</method>
		</remote>
	</editor>
</tpl>
